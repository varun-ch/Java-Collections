package com.cloudwick.arrays;

import java.util.Arrays;

public class ArrayMethods {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr[] =new int[10];
		
		for(int i = 0; i < 10; i++)
		      arr[i] = -3 * i;
		
		Arrays.sort(arr);
		for(int i=0; i<arr.length();i++)
			System.out.println(arr[i]);
	}

}
